{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "queens-game",
	"version": "0.1.0",
	"identifier": "com.example.tauri",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build"
	},
	"app": {
		"windows": [
			{
				"title": "queens-game",
				"fullscreen": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/wQ.ico"
		]
	}
}
